<p id="notice"><%= notice %></p>

<h1>Homes</h1>

<table>
  <thead>
    <tr>
      <th>Cityid</th>
      <th>Date</th>
      <th>Start</th>
      <th>End</th>
      <th>Peoplenum</th>
      <th>Teacherid</th>
      <th>Cafeid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @homes.each do |home| %>
      <tr>
        <td><%= home.cityId %></td>
        <td><%= home.date %></td>
        <td><%= home.start %></td>
        <td><%= home.end %></td>
        <td><%= home.peopleNum %></td>
        <td><%= home.teacherId %></td>
        <td><%= home.cafeId %></td>
        <td><%= link_to 'Show', home %></td>
        <td><%= link_to 'Edit', edit_home_path(home) %></td>
        <td><%= link_to 'Destroy', home, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Home', new_home_path %>
