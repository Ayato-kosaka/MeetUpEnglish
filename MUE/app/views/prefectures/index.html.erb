<p id="notice"><%= notice %></p>

<h1>Prefectures</h1>

<%= link_to 'New Prefecture', new_prefecture_path %>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <!-- <th>Regionid</th> -->
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @prefectures.each do |prefecture| %>
      <tr>
        <td><%= prefecture.id %></td>
        <td><%= prefecture.name %></td>
        <!-- <td><%= Region.find(prefecture.region_id).name %></td> -->
        <td><%= link_to 'Edit', edit_prefecture_path(prefecture) %></td>
        <td><%= link_to 'Destroy', prefecture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
